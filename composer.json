{
    "name": "oxygen/preferences",
    "description": "Preferences Framework for Oxygen",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "c.chamberlain@iinet.net.au"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "oxygen/core": "dev-master",
        "illuminate/support": "~5.1.0"
    },
    "autoload": {
        "psr-4": {
            "Oxygen\\Preferences\\": "src/"
        }
    },
    "extra": {
        "title": "Oxygen Preferences",
        "readme": "oxygen.md",
        "changes": "changes.md",
        "icon": "icon.png",
        "providers": [
            {
                "name": "Preferences",
                "class": "Oxygen\\Preferences\\PreferencesServiceProvider",
                "description": "Adds the Preferences Framework"
            }
        ]
    }
}
